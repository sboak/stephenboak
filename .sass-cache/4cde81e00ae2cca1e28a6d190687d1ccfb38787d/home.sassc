o:Sass::Tree::RootNode	:@has_childrenT:@template"Œ&@import common

h1
  font-family: 'museo-slab-1','museo-slab-2', sans-serif
  font-size: 225%
  font-weight: bold
  line-height: 1.25
h2
  color: gray
  font-family: 'museo-slab-1','museo-slab-2', sans-serif
  font-size: 150%
  font-weight: light
  margin-bottom: .25em
a
  color: $sb_green
  text-decoration: none
  &:hover
    cursor: pointer
    text-decoration:  underline

///////////////////////////////////////////////////////////
// MAIN NAVIGATION BAR ////////////////////////////////////
///////////////////////////////////////////////////////////
nav
  background-image: url('/images/logo.png')
  background-position: 0 0
  background-repeat: no-repeat
  display: block
  height: 80px
  min-width: 42px
  margin: 30px auto
  width: $container_width
  //padding-left: 60px
  ul
    float: right
    font-family: 'museo-slab-1','museo-slab-2', sans-serif
    list-style: none
    li
      display: inline-block
      float: left
      font-size: 110%
      padding: 0 20px
      text-align: right
      a
        color: inherit
        font-weight: bold
        &:hover
          color: $sb_green
          text-decoration: none
    .blog
      @include round(4px)
      background-color: $sb_green
      padding: 3px 8px
      &:hover
        color: yellow

///////////////////////////////////////////////////////////
// HEADLINE SECTION ///////////////////////////////////////
///////////////////////////////////////////////////////////

img.me
  -webkit-transform: rotate(5deg)
  -moz-transform: rotate(5deg)
  -webkit-box-shadow: 0 1px 3px rgba(128,128,128,.5)
  -moz-box-shadow: 0 1px 3px rgba(128,128,128,.5)
  box-shadow: 0 1px 3px rgba(128,128,128,.5)
  border: 5px solid white
  float: right
  position: relative
  right: -30px
  top: -130px
  z-index: 3
img.me_b
  -webkit-transform: rotate(2deg)
  -moz-transform: rotate(2deg)
  -webkit-box-shadow: 0 1px 3px rgba(128,128,128,.5)
  -moz-box-shadow: 0 1px 3px rgba(128,128,128,.5)
  box-shadow: 0 1px 3px rgba(128,128,128,.5)
  border: 5px solid white
  float: right
  position: relative
  right: -200px
  top: 50px
  z-index: 2
img.me_ski
  -webkit-transform: rotate(-4deg)
  -moz-transform: rotate(-4deg)
  -webkit-box-shadow: 0 1px 3px rgba(128,128,128,.5)
  -moz-box-shadow: 0 1px 3px rgba(128,128,128,.5)
  box-shadow: 0 1px 3px rgba(128,128,128,.5)
  border: 5px solid white
  float: right
  position: relative
  right: -360px
  top: -160px
  z-index: 1

.headline
  h1
    margin-bottom: .5em
    width: 650px
#social
  @include graygrad
  padding: .5em 1em
  margin: 3em 0
  width: 400px
  .network
    color: $sb_green
    display: inline-block
    font-size: 90%
    margin-left: .5em
  .social_link
    @include round(18px)
    -webkit-transition: background-color 250ms
    -moz-transition: background-color 250ms
    -o-transition: background-color 250ms
    -ms-transition: background-color 250ms
    transition: background-color 250ms
    background-color: gray
    background-repeat: no-repeat
    background-position: 50% 50%
    display: inline-block
    height: 36px
    margin-right: 15px
    position: relative
    width: 36px
    &:hover
      background-color: $sb_green
      text-decoration: none
  #twitter
    background-image: url('/images/logo-twitter.png')
  #facebook
    background-image: url('/images/logo-facebook.png')
  #linkedin
    background-image: url('/images/logo-linkedin.png')
  #github
    background-image: url('/images/logo-github.png')
  #dribbble
    background-image: url('/images/logo-dribbble.png')


///////////////////////////////////////////////////////////
// PORTFOLIO SECTION //////////////////////////////////////
///////////////////////////////////////////////////////////
#portfolio
  figure
    .name
      color: gray
      line-height: 1.2
      padding: 3px 5px
    .desc
      display: block
      font-size: 90%
      line-height: 1.2
      padding: 3px 5px
  .banner
    -webkit-transition: border 250ms
    -moz-transition: border 250ms
    -o-transition: border 250ms
    -ms-transition: border 250ms
    transition: border 250ms
    background-color: $sb_dark_gray
    border: 5px solid $sb_dark_gray
    display: inline-block
    height: 200px
    width: 940px
    margin-bottom: 10px
    &:hover
      border-color: $sb_green
    a
      img
        height: 140px
        width: 940px
  .thumb
    -webkit-transition: border 250ms
    -moz-transition: border 250ms
    -o-transition: border 250ms
    -ms-transition: border 250ms
    transition: border 250ms
    background-color: $sb_dark_gray
    border: 5px solid $sb_dark_gray
    display: inline-block
    height: 320px
    margin-bottom: 10px
    margin-right: 5px
    vertical-align: top
    width: 300px
    &:hover
      border-color: $sb_green
    a
      img
        height: 200px
        width: 300px
      
  .project
    padding: 70px 20px 20px 20px
    h1
      background-color: #444
      color: white
      font-size: 125%
      font-weight: normal
      padding: 10px
      position: absolute
      top: 0
      left: 0
      right: 0
      text-align: center
    figure
      background-color: $sb_dark_gray
      display: inline-block
      margin-bottom: 1em
      padding: 0 0 10px 0
      position: relative
      vertical-align: top
      text-align: center
      width: 100%
      figcaption
        display: block
        line-height: 1.4
        padding: 10px
        text-align: left
      
///////////////////////////////////////////////////////////
// WRITING SECTION ////////////////////////////////////////
///////////////////////////////////////////////////////////
#writing
  .L2
    display: inline-block
    float: left
    margin-right: 15px
    width: 460px
  #tweets
    height: 600px
    width: 460px
    li
      @include graygrad
      margin: .5em 0
      padding: .5em
    img
      float: left
      padding: 0 10px 0 0
    .date
      color: gray
      font-size: 90%
  .tweets
    @include round
    background: black
    padding: 5px
  #posts
    height: 600px
    width: 460px
    li
      @include graygrad
      margin: .5em 0
      padding: .5em
    .date
      display: block
      color: gray


#instagram
  a
    margin-right: 20px
    &:last-child
      margin-right: 0
  .instagram-image
    -webkit-transition: border-color 250ms
    -moz-transition: border-color 250ms
    -o-transition: border-color 250ms
    -ms-transition: border-color 250ms
    transition: border-color 250ms
    border: 5px solid $sb_dark_gray
    display: inline-block
    height: 160px
    width: 160px
    &:hover
      border-color: $sb_green
      
///////////////////////////////////////////////////////////
// ABOUT ME SECTION ///////////////////////////////////////
///////////////////////////////////////////////////////////
#about
  padding-bottom: 80px
  h2
    margin-bottom: .5em    
  ul
    font-size: 1.05em
    list-style: disc
    margin: 1em
    li
      margin: .5em 0
  a.button
    @include round(8px)
    @include greengrad
    -webkit-box-shadow: 0px 2px 2px black
    -moz-box-shadow: 0px 2px 2px black
    box-shadow: 0px 2px 2px black
    border: none
    color: white
    display: block
    font-size: 1.2em
    margin: 1.5em 0
    padding: .3em .5em
    outline: none
    text-align: center
    width: 10.5em
    &:before
      content: url('/images/arrow-down.png')
      margin-right: 10px
      vertical-align: middle
    &:hover
      color: yellow
      text-decoration: none
      &:before
        content: url('/images/arrow-down-hover.png')
  .L2
    display: inline-block
    float: left
    margin-right: 15px
    width: 460px
  #current_chart, #past_chart
    display: block
    height: 260px
    width: 450px
  .current
    background-color: $sb_green
  .past
    background-color: steelblue
  .bar
    color: white
    height: 25px
    margin-bottom: 4px
    padding: 0 4px
    position: relative
    .level
      color: #E5E5E5
      float: right
      font-size: 10px
      font-style: italic
    &:after
      -moz-box-shadow: 0 2px 2px $sb_dark_gray
      -webkit-box-shadow: 0 2px 2px $sb_dark_gray
      box-shadow: 0 2px 2px $sb_dark_gray
      @include round(5px)
      background-color: gray
      color: white
      display: block
      line-height: 1.25
      position: absolute
      top: 0
      left: 100px
      content: attr(description)
      opacity: 0
      padding: 5px
      width: 250px
      z-index: 1
    &:hover
      cursor: pointer
      &:after
        opacity: 1
    
///////////////////////////////////////////////////////////
// FOOTER /////////////////////////////////////////////////
///////////////////////////////////////////////////////////
footer
  background: #111
  width: 100%
  .L2
    display: inline-block
    vertical-align: top
    width: 48%
    h3
      color: #BBB
    dt
      color: #BBB
    dd, p
      margin-bottom: .75em


///////////////////////////////////////////////////////////
// JQUERY OVERLAYS ////////////////////////////////////////
///////////////////////////////////////////////////////////

.overlay-container
  display: none
  height: 100%
  position: fixed
  width: 100%
  z-index: 10
.ui-widget-overlay
  height: 100%
  position: fixed !important
  width: 100%
  z-index: 10
.ui-widget-shadow
  height: 620px
  left: 50%
  margin: -310px 0 0 -510px !important
  padding: 0 !important
  position: fixed !important
  top: 50%
  width: 1021px
  z-index: 10
.ui-widget-content
  background: #111111
  height: 600px
  left: 50%
  margin: -300px 0 0 -500px
  overflow-x: hidden
  overflow-y: scroll
  padding: 0
  position: fixed !important
  top: 50%
  width: 1000px
  z-index: 11
::-webkit-scrollbar
  width: 6px
::-webkit-scrollbar-track
  background: $sb_dark_gray
::-webkit-scrollbar-thumb
  -webkit-border-radius: 3px
  -moz-border-radius: 3px
  -o-border-radius: 3px
  border-radius: 3px
  background: gray
  cursor: pointer




